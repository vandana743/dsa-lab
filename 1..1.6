#include <stdio.h>

int main() {
    int r, c;
    
    // Read the number of rows and columns
    scanf("%d %d", &r, &c);
    
    int arr[r][c];
    int flat_arr[r * c];
    
    // Read the elements into a 1D array first for convenience
    for (int i = 0; i < r * c; i++) {
        scanf("%d", &flat_arr[i]);
    }
    
    // Populate the 2D array from the 1D array
    int k = 0;
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            arr[i][j] = flat_arr[k++];
        }
    }
    
    // Print in Row-Major Order
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            printf("%d ", arr[i][j]);
        }
        printf("\n");
    }
    
    // Print in Column-Major Order
    for (int j = 0; j < c; j++) {
        for (int i = 0; i < r; i++) {
            printf("%d ", arr[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
