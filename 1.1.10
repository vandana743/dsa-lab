#include <stdio.h>

int main() {
    int n, key, i;

    // Read the number of elements
    scanf("%d", &n);

    int arr[n];

    // Read the array elements
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Read the key element to be searched
    scanf("%d", &key);

    int found_index = -1;

    // Perform linear search
    for (i = 0; i < n; i++) {
        if (arr[i] == key) {
            found_index = i;
            break; // Exit the loop on the first occurrence
        }
    }

    // Print the result
    if (found_index != -1) {
        printf("%d\n", found_index);
    } else {
        printf("Not found\n");
    }

    return 0;
}
